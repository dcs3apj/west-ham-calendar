<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:DialogBox ui:field="popupPanel" animationEnabled="true" autoHideEnabled="false" modal="true" glassEnabled="true">
		<g:caption>Google Authentication Required</g:caption>
		<g:HTMLPanel>
			<div>
				Copy and paste the code supplied below.
			</div>
			<div>
				<g:TextBox ui:field="successCode"/>
				<g:Button ui:field="submitButton">Proceed</g:Button>			
			</div>
		</g:HTMLPanel>		
	</g:DialogBox>
</ui:UiBinder>